{
    "name": "behzadsp/remote",
    "description": "Remote SSH access for The Laravel Framework.",
    "license": "MIT",
    "homepage": "https://github.com/behzadsp/remote",
    "support": {
        "issues": "https://github.com/behzadsp/remote/issues",
        "source": "https://github.com/behzadsp/remote"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylorotwell@gmail.com"
        },
        {
            "name": "Behzad Soltanpour",
            "email": "behzad@engineer.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/filesystem": "^6.0|^7.0|^8.0|^9.0",
        "phpseclib/phpseclib": "^2.0"
    },
    "require-dev": {
        "illuminate/console": "^6.0|^7.0|^8.0|^9.0",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~5.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.x-dev"
        },
        "laravel": {
            "providers": [
                "Collective\\Remote\\RemoteServiceProvider"
            ],
            "aliases": {
                "SSH": "Collective\\Remote\\RemoteFacade"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Collective\\Remote\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
